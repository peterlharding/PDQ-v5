CC      = gcc
LIB     = ../../lib
INCL    = ../../lib
EXT     =

#=====================================================================

passport2:	passport2.c $(LIB)/libpdq.a
	${CC} -o passport2 passport2.c -I$(INCL) -L$(LIB) -lpdq -lm
	
#-----------------------------------------------------------

clean:
	-/bin/rm  $(PGM) *.o *.exe *~ *.log *.tst Ch3_*.out

